#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x5555d28f8520 .scope module, "behavior_model_tb" "behavior_model_tb" 2 4;
 .timescale -9 -12;
v0x5555d290dc10_0 .var/i "i", 31 0;
v0x5555d290dd10_0 .net "t_F", 0 0, v0x5555d290da60_0;  1 drivers
v0x5555d290ddd0_0 .var "t_input", 3 0;
L_0x5555d290de70 .part v0x5555d290ddd0_0, 3, 1;
L_0x5555d290df10 .part v0x5555d290ddd0_0, 2, 1;
L_0x5555d290e000 .part v0x5555d290ddd0_0, 1, 1;
L_0x5555d290e0a0 .part v0x5555d290ddd0_0, 0, 1;
S_0x5555d28f86b0 .scope module, "dut" "circuit_behavior" 2 9, 3 3 0, S_0x5555d28f8520;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "F";
    .port_info 1 /INPUT 1 "A";
    .port_info 2 /INPUT 1 "B";
    .port_info 3 /INPUT 1 "C";
    .port_info 4 /INPUT 1 "D";
v0x5555d28f8890_0 .net "A", 0 0, L_0x5555d290de70;  1 drivers
v0x5555d290d840_0 .net "B", 0 0, L_0x5555d290df10;  1 drivers
v0x5555d290d900_0 .net "C", 0 0, L_0x5555d290e000;  1 drivers
v0x5555d290d9a0_0 .net "D", 0 0, L_0x5555d290e0a0;  1 drivers
v0x5555d290da60_0 .var "F", 0 0;
E_0x5555d28f5980 .event anyedge, v0x5555d290d9a0_0, v0x5555d290d900_0, v0x5555d290d840_0, v0x5555d28f8890_0;
    .scope S_0x5555d28f86b0;
T_0 ;
    %wait E_0x5555d28f5980;
    %load/vec4 v0x5555d28f8890_0;
    %load/vec4 v0x5555d290d840_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5555d290d900_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5555d290d9a0_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 10, 0, 4;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 12, 0, 4;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 14, 0, 4;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555d290da60_0, 0, 1;
    %jmp T_0.8;
T_0.8 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5555d28f8520;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5555d290ddd0_0, 0, 4;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5555d290dc10_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5555d290dc10_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %delay 30000, 0;
    %load/vec4 v0x5555d290dc10_0;
    %pad/s 4;
    %store/vec4 v0x5555d290ddd0_0, 0, 4;
    %load/vec4 v0x5555d290dc10_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5555d290dc10_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
    .scope S_0x5555d28f8520;
T_2 ;
    %vpi_call 2 21 "$display", "Written by Max Benedict Chavez" {0 0 0};
    %vpi_call 2 22 "$monitor", "time = %0d ", $time, "A = %b B = %b C = %b D = %b output_F = %b", &PV<v0x5555d290ddd0_0, 3, 1>, &PV<v0x5555d290ddd0_0, 2, 1>, &PV<v0x5555d290ddd0_0, 1, 1>, &PV<v0x5555d290ddd0_0, 0, 1>, v0x5555d290dd10_0 {0 0 0};
    %vpi_call 2 23 "$dumpfile", "RChaM.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
